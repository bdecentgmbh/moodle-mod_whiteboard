{{!
    @template mod_whiteboard/whiteboard

    Display the board into iframe.

    Example context (json):
    {
        "action": "https://example.com/"
    }

}}
<div class="whiteboard-block ">
	{{#url}}
	<iframe width="100%" height="800" src="{{{url}}}" style="border: 1px solid #eee;"></iframe>
	{{/url}}
</div>
